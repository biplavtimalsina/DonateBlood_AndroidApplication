<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lyanglyang.newapp_oct26.Update_info">


    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/update"
        tools:layout_editor_absoluteX="-37dp"
        tools:layout_editor_absoluteY="-30dp"
        android:scaleType="centerCrop"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/updateButtonme"
        android:layout_width="141dp"
        android:layout_height="56dp"
        android:text="Update"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="423dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="18dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="221dp"
        android:layout_height="135dp"
        app:srcCompat="@drawable/heartbeat3"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="430dp"
        android:layout_marginBottom="21dp"
        android:layout_alignBottom="@+id/updateButtonme"
        android:layout_centerHorizontal="true" />

    <EditText
        android:id="@+id/regName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_above="@+id/editText5"
        android:layout_alignStart="@+id/editText5" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="178dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Age"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="155dp"
        android:layout_above="@+id/editText6"
        android:layout_alignStart="@+id/editText6" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Phone Number"
        android:inputType="phone"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="195dp"
        android:layout_marginBottom="13dp"
        android:layout_above="@+id/editText7"
        android:layout_alignStart="@+id/editText7" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Location"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="238dp"
        android:layout_above="@+id/editText8"
        android:layout_alignStart="@+id/editText8" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="336dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Number of times blood donated?"
        android:inputType="number"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="281dp"
        android:layout_above="@+id/editText9"
        android:layout_centerHorizontal="true" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="337dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Last Time blood donated"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="326dp"
        android:layout_above="@+id/imageView3"
        android:layout_alignStart="@+id/editText7" />

</RelativeLayout>
