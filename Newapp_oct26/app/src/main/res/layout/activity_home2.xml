<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lyanglyang.newapp_oct26.Home">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/home"
        tools:layout_editor_absoluteX="-37dp"
        tools:layout_editor_absoluteY="-30dp"
        android:layout_alignParentStart="true"
        android:scaleType="centerCrop"
        android:layout_alignParentTop="true" />

    <Button
        android:id="@+id/butHome_rewards"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rewards"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="447dp"
        android:layout_alignBaseline="@+id/butHome_invite"
        android:layout_alignBottom="@+id/butHome_invite"
        android:layout_toEndOf="@+id/butHome_postRequests" />

    <Button
        android:id="@+id/butHome_postRequests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Post Requests"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_alignTop="@+id/butHome_me"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp" />

    <Button
        android:id="@+id/butHome_seeRequests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="See Requests"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_alignTop="@+id/butHome_me"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="298dp"
        android:layout_height="261dp"
        app:srcCompat="@drawable/map"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="180dp"
        android:layout_marginBottom="50dp"
        android:layout_above="@+id/butHome_rewards"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/butHome_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update"
        tools:layout_editor_absoluteX="271dp"
        tools:layout_editor_absoluteY="447dp"
        android:layout_alignBaseline="@+id/butHome_rewards"
        android:layout_alignBottom="@+id/butHome_rewards"
        android:layout_alignStart="@+id/butHome_postRequests"
        android:layout_marginStart="14dp" />

    <Button
        android:id="@+id/butHome_invite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Invite"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="444dp"
        android:layout_marginBottom="13dp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/butHome_seeRequests"
        android:layout_marginEnd="15dp" />

    <ImageButton
        android:id="@+id/butHome_me"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_my_calendar"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="112dp"
        android:layout_marginBottom="16dp"
        android:layout_above="@+id/imageView5"
        android:layout_alignEnd="@+id/butHome_rewards" />

    <ImageButton
        android:id="@+id/butHome_logOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="26dp"
        app:srcCompat="@android:drawable/ic_lock_power_off"
        tools:layout_editor_absoluteX="325dp"
        tools:layout_editor_absoluteY="44dp"
        android:layout_above="@+id/butHome_postRequests"
        android:layout_toEndOf="@+id/imageView5" />

</RelativeLayout>
